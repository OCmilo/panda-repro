    "build": "tsup ./src"
  },
  "devDependencies": {
    "@park-ui/panda-preset": "^0.37.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "globals": "^15.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "*"
  },
  "dependencies": {
    "@ark-ui/react": "^2.2.3",
    "@pandacss/dev": "^0.37.1",
    "@types/react": "^18.2.79",
    "postcss": "^8.4.38",
    "react": "^18.2.0"
  }
